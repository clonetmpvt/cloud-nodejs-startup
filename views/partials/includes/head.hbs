<style>
    /* roboto-regular - latin */
    @font-face {
        font-family: 'Roboto';
        font-style: normal;
        font-weight: 400;
        src: url('/images/fonts/roboto-v29-latin-regular.eot');
        /* IE9 Compat Modes */
        src: local(''),
            url('/images/fonts/roboto-v29-latin-regular.eot?#iefix') format('embedded-opentype'),
            /* IE6-IE8 */
            url('/images/fonts/roboto-v29-latin-regular.woff2') format('woff2'),
            /* Super Modern Browsers */
            url('/images/fonts/roboto-v29-latin-regular.woff') format('woff'),
            /* Modern Browsers */
            url('/images/fonts/roboto-v29-latin-regular.ttf') format('truetype'),
            /* Safari, Android, iOS */
            url('/images/fonts/roboto-v29-latin-regular.svg#Roboto') format('svg');
        /* Legacy iOS */
    }

    /* roboto-500 - latin */
    @font-face {
        font-family: 'Roboto';
        font-style: normal;
        font-weight: 500;
        src: url('/images/fonts/roboto-v29-latin-500.eot');
        /* IE9 Compat Modes */
        src: local(''),
            url('/images/fonts/roboto-v29-latin-500.eot?#iefix') format('embedded-opentype'),
            /* IE6-IE8 */
            url('/images/fonts/roboto-v29-latin-500.woff2') format('woff2'),
            /* Super Modern Browsers */
            url('/images/fonts/roboto-v29-latin-500.woff') format('woff'),
            /* Modern Browsers */
            url('/images/fonts/roboto-v29-latin-500.ttf') format('truetype'),
            /* Safari, Android, iOS */
            url('/images/fonts/roboto-v29-latin-500.svg#Roboto') format('svg');
        /* Legacy iOS */
    }

    /* roboto-700 - latin */
    @font-face {
        font-family: 'Roboto';
        font-style: normal;
        font-weight: 700;
        src: url('/images/fonts/roboto-v29-latin-700.eot');
        /* IE9 Compat Modes */
        src: local(''),
            url('/images/fonts/roboto-v29-latin-700.eot?#iefix') format('embedded-opentype'),
            /* IE6-IE8 */
            url('/images/fonts/roboto-v29-latin-700.woff2') format('woff2'),
            /* Super Modern Browsers */
            url('/images/fonts/roboto-v29-latin-700.woff') format('woff'),
            /* Modern Browsers */
            url('/images/fonts/roboto-v29-latin-700.ttf') format('truetype'),
            /* Safari, Android, iOS */
            url('/images/fonts/roboto-v29-latin-700.svg#Roboto') format('svg');
        /* Legacy iOS */
    }

    html,
    html *,
    body * {
        font-family: 'Roboto', sans-serif;
    }

    .logo {
        max-width: 5000px !important;
    }

    .theme-3 .navbar-toggler {
        color: #fff;
        padding: 8px 15px;
        background: #e74907;
        position: absolute;
        top: -55px;
        z-index: 9;
        margin-left: -15px;
    }

    @media only screen and (max-width: 1024px) {
        .theme-3 .navbar {
            width: 90%;
        }
    }

    .navbar-dark .navbar-nav .nav-link {
        font-size: 0.9rem;
        text-transform: capitalize;
    }
</style>

