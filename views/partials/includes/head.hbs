<meta charset="utf-8" />
<title>{{ page.seo.title }}</title>
<meta property="description" name="description" content="{{ page.seo.description }}" />
<meta property="keywords" name="keywords" content="{{ page.seo.keywords }}" />
<link rel="canonical" rel="canonical" href="{{page.canonical}}" />
<link rel="manifest" href="{{SITE_URL}}/images/manifest.json" />
<meta name="viewport" content="width=device-width, initial-scale=1" />
<meta data-hid="mobile-web-app-capable" name="mobile-web-app-capable" content="yes" />
<meta data-hid="apple-mobile-web-app-title" name="apple-mobile-web-app-title" content="{{SITE_NAME}}" />
<meta data-hid="theme-color" name="theme-color" content="#553a7d" />
<meta data-hid="og:type" name="og:type" property="og:type" content="website" />
<meta data-hid="og:title" name="og:title" property="og:title" content="{{ page.seo.title }}" />

<meta data-hid="og:site_name" name="og:site_name" property="og:site_name" content="{{SITE_NAME}}" />
<meta name="og:description" property="og:description" content="{{ page.seo.description }}" />
<meta name="og:image" property="og:image" content="{{SITE_URL}}/thumb/small\images/DEFAULT_IMG.webp" />

<link rel="icon" type="image/x-icon" href="{{SITE_URL}}/favicon.ico" />
<link rel="shortcut icon" href="{{SITE_URL}}/thumb/icon_512/images/logo.webp" sizes="512x512" />
<link rel="apple-touch-icon" href="{{SITE_URL}}/thumb/icon_512/images/logo.webp" sizes="512x512" />

{{#if page.ampUrl}}
<link rel="amphtml" href="{{SITE_URL}}/article/{{page.slug}}/amp" />
{{/if}}

<meta name="robots" content="max-image-preview:large">
<!-- Primary Meta Tags -->
<meta name="title" content="{{ page.seo.title }}">

<link rel="alternate" type="application/rss+xml" title="{{SITE_NAME}} &raquo; Feed" href="{{SITE_URL}}/feed" />
<!-- Open Graph / Facebook -->
<meta property="og:type" content="website">
<meta property="og:url" content="{{SITE_URL}}">

<!-- Twitter -->
<meta property="twitter:card" content="summary_large_image">
<meta property="twitter:url" content="{{SITE_URL}}">
<meta property="twitter:title" content="{{ page.seo.title }}">
<meta property="twitter:description" content="{{ page.seo.description }}">
<meta property="twitter:image" content="{{SITE_URL}}/thumb/large\images/DEFAULT_IMG.webp">

{{#if page.json_ld}}
<script type="application/ld+json">
    {{{page.json_ld}}}
</script>
{{/if}}
{{!-- {{> includes/style }} --}}
{{#if page_css}}
<link rel="stylesheet" href="{{SITE_URL}}/images/newsprk/assets/css/{{page_css}}.css">
{{else}}
<link rel="stylesheet" href="{{SITE_URL}}/images/newsprk/assets/css/compress.css">
{{/if}}
