<body>
    <div class="container">


        <div class="row">

            <div class="blog-main col-sm-8">
                <div class="blog-post">
                    <h1 class="blog-post-title">{{ page.seo.title}}</h1>
                    <amp-img src="{{SITE_URL}}{{page.image}}" layout="responsive" width="800" height="600"
                        layout="responsive" alt="{{ page.seo.title}}">
                        <div fallback>offline</div>
                    </amp-img>
                    {{{page.content}}}
                </div>
            </div><!-- /.blog-main -->






            <div class="blog-sidebar col-sm-3 col-sm-offset-1">
                <a href="{{SITE_URL}}">
                    <amp-img src="{{SITE_URL}}/images/logo.webp" layout="responsive" width="300" height="150"
                        alt="{{SITE_NAME}}"></amp-img>
                </a>

                <div class="sidecards">
                    <h2>Popular Articles</h2>
                    {{#each popular}}
                    <a href="{{SITE_URL}}/article/{{slug}}">
                        <amp-img src="{{SITE_URL}}/thumb/medium{{image}}" layout="responsive" width="300" height="200"
                            alt="{{title}}"></amp-img>
                    </a>
                    <h4><a href="{{SITE_URL}}/article/{{slug}}">{{title}}</a></h4>
                    {{/each}}
                </div>

                <div>
                    <h4><a href="{{SITE_URL}}">Latest Posts</a></h4>
                    <ol class="list-unstyled">
                        {{#each latest_20}}
                        <li><a href="{{SITE_URL}}/article/{{slug}}">{{title}}</a></li>
                        {{/each}}
                    </ol>
                </div>

            </div><!-- /.blog-sidebar -->

        </div><!-- /.row -->

        <hr>

        <div class="blog-footer container">
            <p class="text-center">{{#domainShow}}{{/domainShow}}&copy; Copyright 22</p>
        </div>

    </div><!-- /.container -->

